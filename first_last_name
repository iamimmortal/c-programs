#include<stdio.h>
#include<conio.h>
void main()
{
	char f[12],s[10],name[18],i,j,n;
	printf("Enter your first name\n");
	scanf("%s",f);
	printf("Enter your last name\n");
	scanf("%s",s);
	n=strlen(f);
	for(i=0,j=n; f[i]!='\0', ;i++,j++)
	{
		name[i]=f[i];
		name[j]=s[i];
	}
	
	printf("name is:%s",name);
	}
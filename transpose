#include<stdio.h>
#include<conio.h>
void main()
{
	clrscr();
int a[3][3],i,j;
printf("Enter matrix element:\n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
	{
		printf("Enter value for pos %d,%d:",i,j);
		scanf("%d",&a[i][j]);
		}
		}
		printf("Entered value are:\n");
for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
	{
		printf("%d\t",a[i][j]);
		
		}
		printf("\n");
		}
			printf("value after transpose:\n");
for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
	{
		printf("%d\t",a[j][i]);
		
		}
		printf("\n");
		}
}
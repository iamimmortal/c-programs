#include<stdio.h>
int array(int *arr,int);
 int main()
 {
 	int a[15],i,x,n;
 	printf("Enter range number:\n");
     scanf("%d",&n);
 	for(i=0;i<n;i++)
 	{      
 	     printf("Enter value for position:%d\n",i+1);
 			scanf("%d",&a[i]);
 			
 	}
 	x=array(a, n);
 	printf("the sum of number is %d",x);
 
 	
 }
 int array(int *arr,int n)
 {
 	int sum=0,i;
 		for(i=0;i<n;i++)
 	{      
 	     sum=sum+arr[i];
 	     //printf("\nsum=%d",arr[i]);
 			
 	}
 	return sum;
 		//printf("\nsum=%d",arr[i]);
 }
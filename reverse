#include<stdio.h>
#include<conio.h>
void main()
{
	int x[10];
	int i,j,tem,a;
		printf("Enter number\n");
		
	scanf("%d",&a);
	for(i=0;i<a;i++)
	{
		printf("Enter element\n");
		scanf("%d",&x[i]);
		}

//	printf("%d",x);
for(i=0,j=a-1;i<a/2;i++,j--)
{
	tem=x[i];
	x[i]=x[j];
	x[j]=tem;
	

}
for(i=0;i<a;i++)
{
printf("%d\t",x[i]);
		}
}
#include<stdio.h>
#include<conio.h>
void main()
{
	clrscr();
int temp,t1,armstrong=0,n;
	printf("enter a number:\n");
	scanf("%d",&n);
	t1=n;
	while(n!=0)
	{
		temp=n%10;
		armstrong=armstrong+temp*temp*temp;
		n=n/10;
	}
	if(armstrong==t1)
	{
		
		printf("Armstrong number\n");
		}
		else
		{
			printf("Not a armstrong number\n");
	}
}

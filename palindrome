#include "stdio.h"
int main()
{
	char name[10];
	int i,j,f;
	printf("Enter name:");
//	name=malloc(10*sizeof(name));
	scanf("%s",name);
//	printf("hey");
	for(i=0,j=strlen(name)-1;name[i]!='\0';i++,
	j--)
	{
	//	printf("%d",j);
		if(name[i]==name[j])
		{
			//printf("palindrome");
			f=1;
		}
		
	}
	if(f==1)
	{
		printf("\n%s is palindrome",name);
	}
	else
	{
		printf("\n%s is not palindrome",name);
	}
	
}